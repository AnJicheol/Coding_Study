import java.util.*;
class Solution {
    public int[] solution(String[] id_list, String[] report, int k) {
        int[] answer = new int[id_list.length];
        int k_count = 0;
        String[] reporter_target;
        ArrayList<String> reporters = new ArrayList<>();
        ArrayList<String> report_list = new ArrayList<>();
        for (int i = 0; i < report.length; i++) {
           if (!report_list.contains(report[i])) report_list.add(report[i]);
        }
        
        for (String target : id_list) {
            k_count = 0;
            reporters.clear();
            
            for (String reporter_a : report_list) {
                reporter_target = reporter_a.split(" ");
                
                if (target.equals(reporter_target[1])) {
                    
                    k_count++;
                    reporters.add(reporter_target[0]);
                }
            }
            
            if (k_count >= k) {
                
                for (int i = 0; i < id_list.length; i++) {
                    if (reporters.contains(id_list[i])) answer[i]++;
                }
            }
            

        }

        return answer;
    }
}
//신고자들을 찾는데 오류가 있어서
//String에서 contains 를 사용하지 않고
//리스트에 하나씩 담아 요소가 존재하는지 비교해보는 방법으로 변경
